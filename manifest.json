{
    "manifest_version": 3,
    "name": "Google Or Not",
    "version": "1.0",
    "description": "Routes queries to preferred LLM or Google based on query intent",
    "permissions": [
        "webNavigation",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
